
@{
    Layout = "";
}
@Styles.Render("~/Content/IndexCSS")
<link href='https://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>
<link href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" rel="stylesheet" />
@Scripts.Render("~/Scripts/IndexScript")
<script src="~/Content/DatatableGrid/SauchalayRegistrationGrid.js"></script>
<script src="https://cdn.datatables.net/plug-ins/1.12.1/pagination/input.js"></script>

<style>
    .paginate_input {
        width: 10%;
        text-align: center;
    }

    .dataTables_length select {
        padding: 2px 1.8rem 1px 8px !important
    }

    .paginate_button:hover {
        border: 1px solid #0973cb !important;
        color: #fff;
        background: #189cdf !important;
    }

    .dataTables_wrapper .dataTables_paginate .paginate_button.disabled, .dataTables_wrapper .dataTables_paginate .paginate_button.disabled:hover, .dataTables_wrapper .dataTables_paginate .paginate_button.disabled:active {
        cursor: default;
        color: #666 !important;
        border: 1px solid transparent !important;
        background: transparent !important;
        box-shadow: none;
        /*  padding:1%*/
    }
</style>


<body>

    <div class="container">
        <br />
        <div style="width:100%; margin:0 auto;">
            @*<a href="AddSauchalayDetails" class="btn btn-success btn-sm float-right" type="button">Add CT /PT</a>*@
            <label style="display:inline-flex;padding-right:5px" class="float-right">Search:&nbsp;<input type="text" id="s" class="form-control form-control-sm" onkeyup="Search()"></label>
            <table id="demoGrid" class="table table-striped table-bordered dt-responsive nowrap" width="100%" cellspacing="0">
                <thead>
                    @*<tr>
                            <th>Id</th>
                            <th>घर आयडी <br />House ID</th>
                            <th>नाव <br />Name</th>
                            <th>स्कॅनिफाय कोड <br />Scaniffy Code</th>
                            <th>डाउनलोड <br />Download</th>
                            <th>झोन  <br />Zone</th>
                            <th>वार्ड क्रमांक <br />Ward No.</th>
                            <th>क्षेत्र <br />Areas</th>
                            <th>घर क्रमांक <br />House No.</th>
                            <th>मोबाइल <br />Mobile</th>
                            <th>पत्ता <br />Address</th>
                            <th data-sortable="false">क्रिया <br />Action</th>

                        </tr>*@

                    <tr>
                        <th> Id </th>
                        <th> CTPT Id</th>
                        <th> Name</th>
                        <th> Scaniffy Code</th>
                        <th> Download</th>
                        <th> Mobile </th>
                        <th> Type Of Toilet </th>
                        <th> Number Of Seats </th>
                        <th> CTPT Image</th>
                        <th> CTPT QrCode Image</th>
                        <th> Address</th>
                        <th> DateTime</th>
                        <th> Electricity Meter Connection</th>
                        <th> Toilet Categary</th>
                        <th> Prabhag</th>
                        <th data-sortable="false"> Action</th>

                    </tr>
                </thead>
            </table>
        </div>
    </div>


    <div id="myModal_Image" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    <h4 id="header_data" class="modal-title"></h4>
                </div>
                <div class="modal-body">

                    <center>
                        <img class="img-responsive" id="imggg" alt="Photo Not Found">
                        <br />
                        <p> <b id="hide_date">Date: </b><span id="dateData"></span></p>
                        <p> <b id="hide_add">Address: </b><span id="latlongData"></span></p>
                    </center>

                    <div class="modal-footer">
                        <button type="button" class="btn" data-dismiss="modal">Close</button>
                        @*<a href="~/Images/Nagpur/Job/download.pdf" class="btn btn-default" download>Download</a>*@
                    </div>
                </div>

            </div>
        </div>
    </div>

    <div id="snackbar">Image not uploaded...</div>
</body>

