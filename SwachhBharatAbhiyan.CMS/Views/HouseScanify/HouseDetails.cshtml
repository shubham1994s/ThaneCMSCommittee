                    console.log("filename-" + fileName);
                    var link = document.createElement('a');
                    link.href = window.URL.createObjectURL(blob);
                    link.download = fileName;
                    link.click();
                    $('#div_ldr').hide();
                    $('#btnDownload').prop('disabled', false);

                } else {
                    $('#div_ldr').hide();
                    $('#btnDownload').prop('disabled', false);

                }
            }

        };

        req.send();

    }


</script>
